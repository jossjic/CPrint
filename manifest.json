{
    "manifest_version": 2,
    "name": "CFPrint",
    "version": "0.0.0.1",
    "permissions": [
      "system.cpu"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Información de la CPU"
    },
    "icons": {
        "128": "logo_128.png"
      },
    "description": "Extensión informativa del consumo energetico"
 }